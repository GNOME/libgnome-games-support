#. Appears at the top of the dialog, as the heading of the dialog
#: ../games/scores/dialog.vala:58
msgid "Congratulations!"
msgstr "Gratulálok!"

#: ../games/scores/dialog.vala:60
msgid "High Scores"
msgstr "Legmagasabb pontok"

#: ../games/scores/dialog.vala:62
msgid "Best Times"
msgstr "Legjobb idők"

#: ../games/scores/dialog.vala:78
msgid "No scores yet"
msgstr "Még nincsenek pontok"

#: ../games/scores/dialog.vala:82
msgid "Play some games and your scores will show up here."
msgstr "Játsszon pár játékot, és a pontjai itt fognak megjelenni."

#. A column heading in the scores dialog
#: ../games/scores/dialog.vala:136
msgid "Rank"
msgstr "Rang"

#. A column heading in the scores dialog
#: ../games/scores/dialog.vala:145
msgid "Score"
msgstr "Pont"

#: ../games/scores/dialog.vala:147
msgid "Time"
msgstr "Idő"

#. A column heading in the scores dialog
#: ../games/scores/dialog.vala:154
msgid "Player"
msgstr "Játékos"

#. Appears on the top right corner of the dialog. Clicking the button closes the dialog.
#: ../games/scores/dialog.vala:164
msgid "Done"
msgstr "Kész"

#. Time which may be displayed on a scores dialog.
#: ../games/scores/dialog.vala:279
#, c-format
msgid "%d minute"
msgid_plural "%d minutes"
msgstr[0] "%d perc"
msgstr[1] "%d perc"

#. Time which may be displayed on a scores dialog.
#: ../games/scores/dialog.vala:281
#, c-format
msgid "%d second"
msgid_plural "%d seconds"
msgstr[0] "%d másodperc"
msgstr[1] "%d másodperc"

#: ../games/scores/dialog.vala:287
msgid "Your score is the best!"
msgstr "Pontszáma a legjobb!"

#: ../games/scores/dialog.vala:289
msgid "Your score has made the top ten."
msgstr "Pontszáma benne van az első tízben."
